<!--
  Schema definition for proposal metadata (used by software tools).
-->
<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema">

    <xsd:complexType name="Meta">
        <xsd:sequence>
            <xsd:element name="firstAttachment" type="xsd:string" minOccurs ="1" maxOccurs="1"/>
            <xsd:element name="secondAttachment" type="xsd:string" minOccurs ="0" maxOccurs="1"/>
        </xsd:sequence>
        <xsd:attribute name="band3optionChosen" type="xsd:boolean" use="required"/>
        <xsd:attribute name="overrideAffiliate" type="xsd:boolean" use="required"/>
    </xsd:complexType>

</xsd:schema>
